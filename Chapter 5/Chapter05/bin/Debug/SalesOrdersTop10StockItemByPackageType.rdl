  <DataSets>
    <DataSet Name="SalesOrders">
      <Query>
        <DataSourceName>DataSourceDataSourceWideWorldImportersDW</DataSourceName>
        <CommandText>SELECT t2.RowNumber, t2.[Buying Package], t2.[Stock Item], t2.Quantity
FROM (
	SELECT ROW_NUMBER() OVER (PARTITION BY [Buying Package] ORDER BY t1.[Stock Item]) RowNumber
		, t1.[Buying Package], t1.[Stock Item], t1.Quantity
	FROM (
			SELECT  SI.[Buying Package], si.[Stock Item], SUM(o.Quantity) Quantity
			FROM Fact.[Order] o
				INNER JOIN Dimension.[Date] d
					ON d.Date = o.[Order Date Key]
				INNER JOIN Dimension.[Stock Item] si
					ON si.[Stock Item Key] = o.[Stock Item Key]
			GROUP BY SI.[Buying Package], si.[Stock Item]
		) t1
	) t2
WHERE t2.RowNumber &lt; 11</CommandText>
      </Query>
      <Fields>
        <Field Name="RowNumber">
          <DataField>RowNumber</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Buying_Package">
          <DataField>Buying Package</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Stock_Item">
          <DataField>Stock Item</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Quantity">
          <DataField>Quantity</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
