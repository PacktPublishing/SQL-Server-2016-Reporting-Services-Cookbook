  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>WideWorldImporters</DataSourceName>
        <CommandText>SELECT c.[Customer]
     , s.[Description]
     , s.[Invoice Date Key]
     , s.[Total Including Tax]
  FROM [Fact].[Sale] s
  JOIN [Dimension].[Customer] c 
    ON s.[Customer Key] = c.[Customer Key]
 WHERE s.[Invoice Date Key] &gt;= '2016-05-01'
   AND s.[Invoice Date Key] &lt;= '2016-05-31'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Customer">
          <DataField>Customer</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Invoice_Date_Key">
          <DataField>Invoice Date Key</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Total_Including_Tax">
          <DataField>Total Including Tax</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
