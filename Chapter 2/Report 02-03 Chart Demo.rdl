  <DataSets>
    <DataSet Name="SalesByInvoiceMonth">
      <Query>
        <DataSourceName>WideWorldImportersDW</DataSourceName>
        <CommandText>SELECT s.[Invoice Date Key]
     , SUM(s.[Total Including Tax]) AS TotalSalesWithTax
  FROM [Fact].[Sale] s
 GROUP BY s.[Invoice Date Key]
</CommandText>
      </Query>
      <Fields>
        <Field Name="Invoice_Date_Key">
          <DataField>Invoice Date Key</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="TotalSalesWithTax">
          <DataField>TotalSalesWithTax</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="InvoiceDateMY">
          <Value>= MonthName(Month(Fields!Invoice_Date_Key.Value), True) + " " + CSTR(YEAR(Fields!Invoice_Date_Key.Value))</Value>
        </Field>
        <Field Name="InvoiceDateYM">
          <Value>=CSTR(YEAR(Fields!Invoice_Date_Key.Value)) + "-" + Right("00" + CSTR(Month(Fields!Invoice_Date_Key.Value)), 2)</Value>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
