  <DataSets>
    <DataSet Name="Territory">
      <Query>
        <DataSourceName>WideWorldImporters</DataSourceName>
        <CommandText> SELECT DISTINCT [Sales Territory]
   FROM [Dimension].[City]
  ORDER BY [Sales Territory]</CommandText>
      </Query>
      <Fields>
        <Field Name="Sales_Territory">
          <DataField>Sales Territory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="TerritorySalesByYear">
      <Query>
        <DataSourceName>WideWorldImporters</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Territory">
            <Value>=Parameters!Territory.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT c.[Sales Territory]
     , YEAR(s.[Invoice Date Key])   AS [Invoice Year]
     , SUM(s.[Total Excluding Tax]) AS [Total Sales]
  FROM [Fact].[Sale] s
  JOIN [Dimension].[City] c
    ON s.[City Key] = c.[City Key]
 WHERE c.[Sales Territory] IN (@Territory)
 GROUP BY c.[Sales Territory], YEAR(s.[Invoice Date Key])
 ORDER BY c.[Sales Territory], YEAR(s.[Invoice Date Key])</CommandText>
      </Query>
      <Fields>
        <Field Name="Sales_Territory">
          <DataField>Sales Territory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Invoice_Year">
          <DataField>Invoice Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Total_Sales">
          <DataField>Total Sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
