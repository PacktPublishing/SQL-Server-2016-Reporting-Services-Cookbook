  <DataSets>
    <DataSet Name="States">
      <Query>
        <DataSourceName>WideWorldImportersDW</DataSourceName>
        <CommandText>SELECT DISTINCT 
       [State Province]
  FROM [Dimension].[City]
 ORDER BY [State Province]
</CommandText>
      </Query>
      <Fields>
        <Field Name="State_Province">
          <DataField>State Province</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="City">
      <Query>
        <DataSourceName>WideWorldImportersDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@State">
            <Value>=Parameters!State.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT   [City]
  FROM [Dimension].[City]
 WHERE [State Province] = @State
 ORDER BY [City]
</CommandText>
      </Query>
      <Fields>
        <Field Name="City">
          <DataField>City</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="OrderOrSales">
      <Query>
        <DataSourceName>WideWorldImportersDW</DataSourceName>
        <CommandText>= Switch(Parameters!OrderOrSale.Value = "Order",
  "SELECT c.[State Province], c.[City], f.[City Key], f.[Customer Key]" &amp;
     ", f.[Stock Item Key], f.[Salesperson Key], f.[Description], f.[Package]" &amp;
     ", f.[Quantity], f.[Unit Price], f.[Tax Rate], f.[Total Excluding Tax]" &amp;
     ", f.[Tax Amount], f.[Total Including Tax]" &amp;
  "  FROM [Fact].[Order] f" &amp;
  "  JOIN [Dimension].[City] c" &amp;
  "    ON f.[City Key] = c.[City Key]" &amp;
  " WHERE c.[City] = '" &amp; Parameters!City.Value &amp; "' " &amp;
  "   AND c.[State Province] = '" &amp; Parameters!State.Value &amp; "' " 
  , Parameters!OrderOrSale.Value = "Sale",
  "SELECT c.[State Province], c.[City], f.[City Key], f.[Customer Key]" &amp;
     ", f.[Stock Item Key], f.[Salesperson Key], f.[Description], f.[Package]" &amp;
     ", f.[Quantity], f.[Unit Price], f.[Tax Rate], f.[Total Excluding Tax]" &amp;
     ", f.[Tax Amount], f.[Total Including Tax]" &amp;
  "  FROM [Fact].[Sale] f" &amp;
  "  JOIN [Dimension].[City] c" &amp;
  "    ON f.[City Key] = c.[City Key]" &amp;
  " WHERE c.[City] = '" &amp; Parameters!City.Value &amp; "' " &amp;
  "   AND c.[State Province] = '" &amp; Parameters!State.Value &amp; "' " 
  )</CommandText>
      </Query>
