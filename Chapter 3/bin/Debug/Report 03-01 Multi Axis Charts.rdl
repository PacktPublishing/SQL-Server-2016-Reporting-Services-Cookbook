  <DataSets>
    <DataSet Name="SalesTotalsOverTime">
      <Query>
        <DataSourceName>WideWorldImportersDW</DataSourceName>
        <CommandText>SELECT YEAR([Invoice Date Key])   AS InvoiceYear
     , SUM([Total Excluding Tax]) AS TotalExcludingTax
     , SUM([Tax Amount])          AS TotalTaxAmount
  FROM [Fact].[Sale]
 GROUP BY YEAR([Invoice Date Key])</CommandText>
      </Query>
      <Fields>
        <Field Name="InvoiceYear">
          <DataField>InvoiceYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalExcludingTax">
          <DataField>TotalExcludingTax</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalTaxAmount">
          <DataField>TotalTaxAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
