  <DataSets>
    <DataSet Name="DataSetSales">
      <Query>
        <DataSourceName>DataSourceWideWorldImportersDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Date">
            <Value>=Code.GetDate(Parameters!ReportParameterSalesDate.Value)</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        s.[Invoice Date Key], c.Customer, ct.City + ' - ' + ct.[State Province] AS City, e.[Preferred Name], si.[Stock Item], SUM(s.Quantity) AS Quantity, SUM(s.[Total Including Tax]) AS Sales
FROM            Fact.Sale AS s INNER JOIN
                         Dimension.Customer AS c ON c.[Customer Key] = s.[Customer Key] INNER JOIN
                         Dimension.City AS ct ON ct.[City Key] = s.[City Key] INNER JOIN
                         Dimension.Employee AS e ON e.[Employee Key] = s.[Salesperson Key] INNER JOIN
                         Dimension.[Stock Item] AS si ON si.[Stock Item Key] = s.[Stock Item Key]
WHERE        (s.[Invoice Date Key] = Convert(date, @Date))
GROUP BY s.[Invoice Date Key], c.Customer, ct.City + ' - ' + ct.[State Province], e.[Preferred Name], si.[Stock Item]</CommandText>
      </Query>
